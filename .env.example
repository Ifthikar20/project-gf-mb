# Environment Configuration
# DO NOT COMMIT THIS FILE TO VERSION CONTROL
# Copy this to .env and fill in your values

# API Configuration
API_BASE_URL=https://api-dev.betterandbliss.app
CDN_BASE_URL=https://cdn-dev.betterandbliss.app

# API Keys (NEVER commit real keys)
API_KEY=your_api_key_here
FIREBASE_API_KEY=your_firebase_key_here

# Analytics
ANALYTICS_KEY=your_analytics_key_here

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_CRASH_REPORTING=false

# Certificate Pinning (SHA-256 hashes)
API_CERT_HASH_1=sha256/XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
API_CERT_HASH_2=sha256/YYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYYY
